<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HeaderTextView">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:lineSpacingExtra">@dimen/dimen_16dp</item>
        <item name="android:layout_marginStart">@dimen/dimen_26dp</item>
        <item name="android:layout_marginEnd">@dimen/dimen_26dp</item>
    </style>

    <style name="ViewPager">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_0dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:overScrollMode">never</item>
    </style>

    <style name="StepImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_4dp</item>
    </style>

    <style name="TabsLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginBottom">@dimen/dimen_100dp</item>
        <item name="tabIndicatorAnimationMode">linear</item>
        <item name="tabIndicatorHeight">@dimen/dimen_0dp</item>
        <item name="tabRippleColor">@null</item>
        <item name="tabMode">fixed</item>
        <item name="tabGravity">center</item>
    </style>

    <style name="VerificationCompleteScreenTitleTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_marginTop">@dimen/dimen_30dp</item>
        <item name="android:text">@string/thank_you</item>
    </style>

    <style name="LivenessInstructionsTextView">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:lineSpacingExtra">@dimen/dimen_8dp</item>
        <item name="android:layout_marginLeft">@dimen/dimen_20dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_20dp</item>
        <item name="android:layout_marginBottom">@dimen/dimen_30dp</item>
    </style>

    <style name="OnboardingTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:lineSpacingExtra">@dimen/dimen_8dp</item>
        <item name="android:layout_marginLeft">@dimen/dimen_100dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_100dp</item>
    </style>

    <style name="VerificationCompleteScreenTextTextView">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/verification_successful</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/dimen_90dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_90dp</item>
        <item name="android:lineSpacingExtra">@dimen/dimen_8dp</item>
    </style>

    <style name="VerificationExpiredScreenTextTextView">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/please_contact_your_compliance_officer</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:lineSpacingExtra">@dimen/dimen_8dp</item>
        <item name="android:layout_marginLeft">@dimen/dimen_100dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_40dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_100dp</item>
    </style>

    <style name="StepTextTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Button">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:theme">@style/Theme.MobileSDKAndroid</item>
        <item name="android:letterSpacing">@dimen/float_0</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="RequirementsTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/requirements</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingLeft">@dimen/dimen_0dp</item>
        <item name="android:paddingRight">@dimen/dimen_0dp</item>
        <item name="android:paddingBottom">@dimen/dimen_4dp</item>
    </style>

    <style name="UploadedTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:drawablePadding">@dimen/dimen_4dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="PoiExampleTextView">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/poi_example_instructions</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">@dimen/dimen_16dp</item>
        <item name="android:layout_marginLeft">@dimen/dimen_60dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_60dp</item>
    </style>

    <style name="PreviewView">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">@dimen/dimen_0dp</item>
        <item name="android:elevation">@dimen/negative_elevation</item>
    </style>

    <style name="PreviewImageView">
        <item name="android:layout_width">@dimen/dimen_0dp</item>
        <item name="android:layout_height">@dimen/dimen_0dp</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:elevation">@dimen/negative_elevation</item>
    </style>

    <style name="PoweredByDataspikeImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/powered_by_dataspike</item>
        <item name="android:layout_marginBottom">@dimen/dimen_30dp</item>
    </style>

    <style name="VerificationCompleteImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dimen_50dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_marginTop">@dimen/dimen_100dp</item>
    </style>

    <style name="CameraButtonImageView">
        <item name="android:layout_width">@dimen/dimen_60dp</item>
        <item name="android:layout_height">@dimen/dimen_60dp</item>
        <item name="android:foreground">@drawable/camera_button_ripple</item>
        <item name="android:src">@drawable/camera_button</item>
        <item name="android:tint">@color/white_color</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="SwitchCameraButtonImageView">
        <item name="android:layout_width">@dimen/dimen_30dp</item>
        <item name="android:layout_height">@dimen/dimen_30dp</item>
        <item name="android:src">@drawable/camera_switch</item>
        <item name="android:tint">@color/white_color</item>
        <item name="android:layout_marginEnd">@dimen/dimen_20dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="OverlayView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:elevation">@dimen/negative_elevation</item>
    </style>

    <style name="TimerTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:drawableStart">@drawable/timer_clock</item>
        <item name="android:drawablePadding">@dimen/dimen_4dp</item>
        <item name="android:paddingStart">@dimen/dimen_10dp</item>
        <item name="android:paddingEnd">@dimen/dimen_10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="SelectCountryTitleTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/select_country</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_marginLeft">@dimen/dimen_16dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
    </style>

    <style name="SelectCountryRecyclerView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_0dp</item>
        <item name="android:layout_marginLeft">@dimen/dimen_16dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_16dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_16dp</item>
        <item name="android:paddingBottom">@dimen/dimen_100dp</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:overScrollMode">never</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="SelectCountryTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/dimen_10dp</item>
    </style>

    <style name="StepsLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
    </style>

    <style name="ScrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_0dp</item>
        <item name="android:overScrollMode">never</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
    </style>

    <style name="SearchBarEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawableEnd">@drawable/search_icon</item>
        <item name="android:hint">@string/search</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
        <item name="android:layout_marginLeft">@dimen/dimen_16dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_16dp</item>
        <item name="android:paddingLeft">@dimen/dimen_10dp</item>
        <item name="android:paddingTop">@dimen/dimen_10dp</item>
        <item name="android:paddingBottom">@dimen/dimen_10dp</item>
        <item name="android:paddingRight">@dimen/dimen_10dp</item>
    </style>

    <style name="ArrowImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_26dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_40dp</item>
        <item name="android:paddingTop">@dimen/dimen_16dp</item>
        <item name="android:src">@drawable/back_button</item>
    </style>

    <style name="CountryFlagImageView">
        <item name="android:layout_width">@dimen/dimen_32dp</item>
        <item name="android:layout_height">@dimen/dimen_24dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/dimen_10dp</item>
    </style>

    <style name="VerificationExpiredImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dimen_100dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_marginTop">@dimen/dimen_40dp</item>
    </style>

    <style name="OnboardingImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dimen_150dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="DocumentPreviewImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_gravity">center|top</item>
        <item name="android:layout_marginTop">@dimen/dimen_20dp</item>
        <item name="android:layout_marginLeft">@dimen/dimen_20dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_20dp</item>
    </style>

    <style name="PoiExampleImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dimen_150dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_marginTop">@dimen/dimen_40dp</item>
    </style>

    <style name="PoaExampleImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dimen_250dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_marginTop">@dimen/dimen_30dp</item>
    </style>

    <style name="ProgressBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dimen_0dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_weight">@dimen/float_1</item>
        <item name="android:indeterminateTint">@color/main_color</item>
    </style>

    <style name="AppCompatCheckBox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_minus_5dp</item>
        <item name="android:layout_marginBottom">@dimen/dimen_minus_5dp</item>
        <item name="android:layout_marginStart">@dimen/dimen_16dp</item>
        <item name="android:layout_marginEnd">@dimen/dimen_minus_15dp</item>
    </style>

    <style name="AppCompatCheckBoxText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_minus_5dp</item>
    </style>

    <style name="WebView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_0dp</item>
    </style>

</resources>